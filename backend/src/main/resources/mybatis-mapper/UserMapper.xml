<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cf.spacetaste.backend.mapper.UserMapper">

    <insert id="create" useGeneratedKeys="true" keyProperty="user.id">
        INSERT INTO
            user(nickname, created_by, updated_by)
        VALUES
            (#{user.nickname}, #{user.id}, #{user.id})
    </insert>

</mapper>